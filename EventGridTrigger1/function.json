{
  "bindings": [
    {
      "type": "eventGridTrigger",
      "name": "eventGridEvent",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "event-logs/{sys.randguid}.txt",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}