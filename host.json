{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",
    "console": {
      "isEnabled": true,
      "level": "Information"
    },
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "retry": {
    "delayInterval": "00:00:02",
    "maxRetryCount": 5
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "managedDependency": {
    "enabled": true
  }
}